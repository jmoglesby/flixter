<div class=" col-10 offset-1">
  <div class="booyah-box">
    <h1>Add a new course</h1>
    <%= simple_form_for @course, url: instructor_courses_path do |f| %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :cost %>
      <br/>
      <%= f.submit 'Create', class: 'btn btn-warning' %>
    <% end %>
    <br/>
  </div>
</div>